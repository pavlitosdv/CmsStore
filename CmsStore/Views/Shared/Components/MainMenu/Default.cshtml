@model IEnumerable<Page>

@foreach (var item in Model)
{
    @if (item.Slug != "home") // we do not want to display the home as there is the deafult one in the Layout
    {
        <li class="nav-item">
            <a class="nav-link text-dark" asp-area="" asp-controller="Pages" asp-action="Page" asp-route-slug="@item.Slug">@item.Title</a>
        </li>
    }
}